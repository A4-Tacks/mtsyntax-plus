[[a]] = /a:/ [[]]
[[b]] = /b:/ [[]]
[[c]] = /(c):/ [[]] $1: "c"
[[d]] = [[]] /:(d)/ $1: "d"

[[a]]
[[b]]
x = /x/
[[c]]
y = /y/
[[d]]
z = /(z)/ // 模板并不具备'本地', 所以指定组会偏移, 不对能
foo := @y
bar := @z
-- end --
"x": /a:b:x/
"y": /(c):y/
"z": /(z):(d)/
"foo": {
    match: include("y")
    1: "c"
}
"bar": {
    match: include("z")
    2: "d"
}
